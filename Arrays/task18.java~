import java.util.Scanner;
public class task18
{
  public static void main(String[]args)
  {
    Scanner sc=new Scanner(System.in);
    int a[]=new int[15];
    int a2[]=new int[10];
    for(int c=0;c<a.length;c++){
      System.out.println("Enter a num with in 0-9");
      a[c]=sc.nextInt();
      a2[a[c]]++;
    }
    for(int c=0;c<a2.length;c++){
      System.out.println(c+"entered"+a2[c]+"times");
    }
  }
}