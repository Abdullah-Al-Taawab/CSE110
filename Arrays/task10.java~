import java.util.Scanner;
public class task10
{
  public static void main(String [] args)
  {
    Scanner sc=new Scanner(System.in);
    int a[]=new int[5];
    int sum=0;
    int max=a[0];
    for(int c=0;c<a.length;c++){
      System.out.println("Enter a num");
      a[c]=sc.nextInt();
    }
    for(int c=0;c<a.length;c++){
      for(int i=0;i<a.length;i++){
        if(a[c]>a[i]){
          max=a[c];
        }
      }
    }
  System.out.print(max);
    }
  }
